/*Swapping numbers  */
 #include<stdio.h>
int main(){
    int temp,a,b;
    printf("Enter a value of a: ");
    scanf("%d",&a);
    printf("Enter a value of b: ");
    scanf("%d",&b);
    printf("Before swapping: \n");
    printf("\na = %d\n",a);
    printf("b = %d\n",b);
    temp = a;
    a = b; 
    b = temp;
    printf("After swapping: \n");
    printf("a = %d\n",a);
    printf("b = %d\n",b);
    return 0;
}
