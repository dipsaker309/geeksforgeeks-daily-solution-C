#include <stdio.h>

int main() {
    char str1[] = "Hello, C!";
    char str2[50];  // Make sure it's large enough

    int i = 0;
    while (str1[i] != '\0') {
        str2[i] = str1[i];
        i++;
    }
    str2[i] = '\0';  // Add null terminator

    printf("Original: %s\n", str1);
    printf("Copied: %s\n", str2);

    return 0;
}
