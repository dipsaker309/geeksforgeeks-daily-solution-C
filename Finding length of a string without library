/*Count the length of a string without using string function*/
#include<stdio.h>
#include<string.h>
int main()
{
    char str[100] ;
    printf("Enter a string : ");
    fgets(str , sizeof(str) , stdin);
    int length = 0;
    while(str[length] != 0){
        length++;
    }
    printf("The size of the string %s is : %d", str , length);
    return 0;
}
