/*Count the length of a string without using string function*/
#include<stdio.h>
#include<string.h>
int main()
{
    char original[100] ;
    printf("Enter a string : ");
    fgets(original , sizeof(original) , stdin);
    int length = 0;
    while(original[length] != 0){
        length++;
    }
    printf("The size of the string %s is : %d", original , length);
    return 0;
}
