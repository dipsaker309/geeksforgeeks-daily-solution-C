/*Count Array elements(By passing array to function)*/
#include<stdio.h>
int countarr(int arr[], int size);
int main()
{
    int arr[] = {2 , 5 , 6 , 7 , 8 , 9 , 10 };
    int size = sizeof(arr) / sizeof(arr[0]);
    printf("The total amount of arr is : %d \n",countarr(arr , size));
    return 0;
}
int countarr(int arr[], int size)
{
    return size;
}
