/*Find LCM of two numbers  */
// Using Euclidean algorithm
#include <stdio.h>
   int gcd(int a,int b)
    {
        int temp;
        while(b != 0)
        {
            temp = b;
            b = a % b;
            a = temp;

        } 
        return a;
    }
int main()
{
    int a , b , i;
    printf("Enter value of a number: ");
    scanf("%d",&a);
    printf("Multiples of %d \n",a);
    for(i = 1 ; i <= 10; i++)
    {
        printf("%d ", a * i);
    }
    printf("\nEnter value of another number: ");
    scanf("%d",&b);
    printf("Multiples of %d : \n",b);
    for(i = 1; i <= 10 ; i++)
    {
        printf("%d ",b * i);
    }
    printf("\n ");
    int lcm = (a * b) / gcd(a,b);
    printf("LCM of %d and %d is : %d ", a , b , lcm);
}
